{
  "name": "webext-require",
  "displayName": "Web Require",
  "version": "0.0.4",
  "description": "An extension to require npm packages in browser dev-tools.",
  "author": "shengfeng.xu",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "npm run build && plasmo package",
    "release": "bumpp"
  },
  "dependencies": {
    "bumpp": "^8.2.1",
    "craie": "^0.0.7",
    "plasmo": "0.61.5"
  },
  "devDependencies": {
    "@types/chrome": "0.0.206",
    "@types/node": "18.11.18",
    "typescript": "4.9.4"
  },
  "manifest": {
    "manifest_version": 2,
    "host_permissions": [
      "*://*/*"
    ]
  }
}
